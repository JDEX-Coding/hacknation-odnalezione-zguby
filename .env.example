# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin123
RABBITMQ_VHOST=/
RABBITMQ_EXCHANGE=lost-found.events
RABBITMQ_QUEUE_INGEST=q.lost-items.ingest
RABBITMQ_QUEUE_PUBLISH=q.lost-items.publish

QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_COLLECTION_NAME=lost-items

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=lost-items-images
MINIO_USE_SSL=false
MINIO_REGION=us-east-1

# External Vision API Configuration
VISION_API_KEY=your-openai-api-key-here
VISION_API_ENDPOINT=https://api.openai.com/v1
VISION_MODEL=gpt-4o

# dane.gov.pl API Configuration
DANEGOV_API_ENDPOINT=https://api.dane.gov.pl
DANEGOV_API_KEY=your-danegov-api-key-here
DANEGOV_DATASET_ID=your-dataset-id

# Service A (Gateway) Configuration
GATEWAY_PORT=8080
GATEWAY_HOST=0.0.0.0

# Service B (AI Worker) Configuration
WORKER_CONCURRENCY=4
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
CLIP_MODEL=openai/clip-vit-base-patch32

# Service C (Publisher) Configuration
PUBLISHER_PORT=8082
PUBLISHER_RETRY_ATTEMPTS=3
PUBLISHER_RETRY_DELAY=5

# General Configuration
LOG_LEVEL=info
ENVIRONMENT=development
